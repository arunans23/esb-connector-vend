<component name="vend-supplier" type="synapse/template">
    <subComponents>
        <component name="createSupplier">
            <file>createSupplier.xml</file>
            <description>Create a new supplier in Vend account using the provided Parameters.</description>
        </component>
        <component name="getSupplier">
            <file>getSupplier.xml</file>
            <description>Retrieve details of a supplier identified by the provided supplier ID.</description>
        </component>
        <component name="listSuppliers">
            <file>listSuppliers.xml</file>
            <description>Retrieve a list of suppliers from Vend account.</description>
        </component>
    </subComponents>
</component>
